@ARTICLE{Johnson2012-hu,
  title    = "{Species diversity reduces parasite infection through
              cross-generational effects on host abundance}",
  author   = "Johnson, Pieter T J and Preston, Daniel L and Hoverman, Jason T
              and Henderson, Jeremy S and Paull, Sara H and Richgels, Katherine
              L D and Redmond, Miranda D",
  abstract = "With growing interest in the effects of biodiversity on disease,
